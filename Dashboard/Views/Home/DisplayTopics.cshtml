@model IEnumerable<Dashboard.Models.Topic>

    <div class="col-lg-12">
        @foreach (var topic in Model)
            {
            <div class="panel panel-default">
                <div class="panel-heading">
                    @topic.Title
                </div>
                <div class="panel-body">
                    <p>@topic.Body</p>
                </div>
                <div class="panel-footer">
                    <small class="text-muted"><i class="fa fa-clock-o"></i> @topic.Created</small>
                </div>
            </div>
        }
    </div>

