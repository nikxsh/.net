@model IEnumerable<Dashboard.Models.Topic>

<div class="col-lg-12">
    @foreach (var topic in Model)
    {
        <div class="panel panel-default">
            <div class="panel-heading">
                @Html.ActionLink(topic.Title, "GetTopics", new { topicId = topic.Id })
            </div>
        </div>
    }
</div>